<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
    <cron>
        <url>/trigger?cmd=enable</url>
        <description>enable poll </description>
        <schedule>every wednesday 8:00</schedule>
        <timezone>Asia/Seoul</timezone>
    </cron>
    <cron>
        <url>/trigger?cmd=disable</url>
        <description>close poll </description>
        <schedule>every wednesday 15:00</schedule>
        <timezone>Asia/Seoul</timezone>
    </cron>
    <cron>
        <url>/trigger?cmd=enable&amp;key=nosamo</url>
        <description>enable poll </description>
        <schedule>every thursday 08:00</schedule>
        <timezone>Asia/Seoul</timezone>
    </cron>
    <cron>
        <url>/trigger?cmd=disable&amp;key=nosamo</url>
        <description>close poll </description>
        <schedule>every thursday 15:00</schedule>
        <timezone>Asia/Seoul</timezone>
    </cron>
</cronentries>