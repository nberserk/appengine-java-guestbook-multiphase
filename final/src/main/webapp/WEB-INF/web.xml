<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>vote</servlet-name>
        <servlet-class>com.nberserk.gae.lessontable.tennis.VoteServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>table</servlet-name>
        <servlet-class>com.nberserk.gae.lessontable.tennis.TableServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>tt_vote</servlet-name>
        <servlet-class>com.nberserk.gae.lessontable.tt.VoteServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>tt_trigger</servlet-name>
        <servlet-class>com.nberserk.gae.lessontable.tt.TriggerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>nosamo</servlet-name>
        <servlet-class>com.nberserk.gae.lessontable.tt.NosamoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>vote</servlet-name>
        <url-pattern>/vote</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>table</servlet-name>
        <url-pattern>/table</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>tt_vote</servlet-name>
        <url-pattern>/tt_vote</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>tt_trigger</servlet-name>
        <url-pattern>/trigger</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>nosamo</servlet-name>
        <url-pattern>/nosamo</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>lesson.html</welcome-file>
    </welcome-file-list>
</web-app>
